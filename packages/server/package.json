    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "mongodb-memory-server": "^9.1.6",
    "supertest": "^6.3.4",
