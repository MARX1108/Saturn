{
  "name": "saturn",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "test:server": "yarn workspace @saturn/server test",
    "test:server:coverage": "yarn workspace @saturn/server test --coverage",
    "lint": "yarn workspace @saturn/server lint",
    "lint:fix": "yarn workspace @saturn/server lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "yarn workspace @saturn/server tsc --noEmit",
    "typecheck:server": "yarn workspace @saturn/server tsc --noEmit",
    "validate": "yarn lint && yarn typecheck && yarn test --passWithNoTests",
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test:contract": "jest --testMatch '**/src/**/*.contract.test.ts'"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/morgan": "^1.9.9",
    "@types/serialize-error": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.31.0"
  },
  "lint-staged": {
    "packages/**/*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{js,jsx,json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.3.11",
    "@react-navigation/native": "^7.1.7",
    "@react-navigation/stack": "^7.3.0",
    "metro": "^0.82.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0"
  }
}
